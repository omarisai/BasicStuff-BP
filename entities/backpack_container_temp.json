				}
			},
			"despawn": {
				"minecraft:instant_despawn": {}
			},
			"open": {
				"minecraft:entity_sensor": {
					"sensor_range": 2,
					"relative_range": false,
					"minimum_count": 1,
					"event_filters": {
						"any_of": [
							{
								"all_of": [
									{
										"test": "is_family",
										"subject": "other",
										"value": "player"
									},
									{
										"test": "has_container_open",
										"operator": "!=",
										"subject": "other"
									}
								]
							}
						]
					},
					"event": "close"
				}
			},
			"close": {
				"minecraft:entity_sensor": {
