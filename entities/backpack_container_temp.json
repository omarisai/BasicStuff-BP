		},
		"component_groups": {
			"timeout": {
				"minecraft:timer": {
					"looping": false,
					"time": 3,
					"time_down_event": {
						"event": "despawn2"
					}
				}
			},
			"reset_health": {
				"minecraft:health": {
					"value": 9,
					"max": 9,
					"min": 1
				},
